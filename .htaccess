RewriteEngine on
#RewriteBase /
#RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
#RewriteRule ^(.*)$ https://%1/$1 [R=301,L]
RewriteRule ^index.html index.php
RewriteRule ^the-loai/([^/]+).html category.php?blank=$1
RewriteRule ^chi-tiet/([^/]+).html view.php?blank=$1
RewriteRule ^page/([^/]+).html page.php?blank=$1
RewriteRule ^tim-kiem.html timkiem.php
RewriteRule ^dang-nhap.html dangnhap.php
RewriteRule ^dang-ky.html dangky.php
RewriteRule ^dang-xuat.html dangxuat.php
RewriteRule ^thong-tin.html thongtin.php
RewriteRule ^quen-mat-khau.html quen_matkhau.php
RewriteRule ^lich-su-nap.html lich_su_nap.php
RewriteRule ^sua-thong-tin.html sua_thongtin.php
RewriteRule ^doi-mat-khau.html doi_matkhau.php
RewriteRule ^nap-the.html napthe.php
RewriteRule ^trang-chu.html home.php
RewriteRule ^tai-game.html taigame.php
